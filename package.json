{
  "name": "slowpitched-react",
  "version": "0.0.1",
  "description": "Pitch Height Tracker Pro - Real-time pitch tracking using computer vision",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "expo start --clear",
    "start": "expo start",
    "build": "eas build --platform all --non-interactive",
    "build:ios": "eas build --platform ios --non-interactive",
    "build:android": "eas build --platform android --non-interactive",
    "build:preview": "eas build --profile preview --platform all --non-interactive",
    "build:preview:ios": "eas build --profile preview --platform ios --non-interactive",
    "build:preview:android": "eas build --profile preview --platform android --non-interactive",
    "build:production": "eas build --profile production --platform all --non-interactive",
    "build:production:ios": "eas build --profile production --platform ios --non-interactive",
    "build:production:android": "eas build --profile production --platform android --non-interactive",
    "web": "expo start --web",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "test": "jest --testPathPattern=\"__tests__\"",
    "test:watch": "jest --testPathPattern=\"__tests__\" --watch",
    "test:coverage": "jest --testPathPattern=\"__tests__\" --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "setup": "npm install && npm run build",
    "clean": "rm -rf node_modules dist .expo"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/native": "^7.1.0",
    "@react-navigation/stack": "^7.4.0",
    "expo": "~53.0.0",
    "expo-av": "~15.1.7",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.7",
    "expo-device": "~7.1.4",
    "expo-notifications": "~0.31.0",
    "expo-sharing": "~13.1.5",
    "expo-sqlite": "^14.0.6",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-vision-camera": "^3.9.2",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react-native": "^13.3.0",
    "@types/jest": "29.5.14",
    "@types/react": "~19.0.14",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-native": "^5.0.0",
    "globals": "^16.4.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.10",
    "prettier": "^3.6.0",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3"
  },
  "pnpm": {
    "overrides": {
      "metro": "^0.82.0",
      "metro-config": "^0.82.0",
      "metro-core": "^0.82.0",
      "metro-runtime": "^0.82.0"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "pitch-tracking",
    "baseball",
    "softball",
    "computer-vision",
    "react-native",
    "expo"
  ],
  "author": "Pitch Height Tracker Team",
  "license": "MIT"
}
